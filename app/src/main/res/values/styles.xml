<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimary</item>

        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>



    </style>

    <style name="FullScreen" parent="AppTheme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>

    </style>

    <style name="PreTheme" parent="FullScreen">
        <item name="android:windowBackground">@color/blue3</item>

    </style>
    <style name="LoginTheme" parent="AppTheme">
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowTranslucentNavigation">true</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>

    </style>
    <style name="LoginTheme1" parent="AppTheme">
        <item name="android:windowTranslucentStatus">true</item>

    </style>

    <style name="StarProgress1" parent="Widget.AppCompat.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/prograss_drawable1</item>
        <item name="android:minHeight">40dp</item>
    </style>


</resources>
